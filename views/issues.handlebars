
<div id="header">
    <a href="/">
        <h1 id="siteName">OPN<span>*</span>SRC</h1>
    </a>

    <br>
    <h2>{{repo}}</h2>
    <p>Here are some projects you can help with. Get on it, Champ.</p>
</div>

</header>



<br>

<div class="container">
    {{#each issues}}

      <div class="container" id="issueCard">
        <div class="card">
            <a class="card-link" href={{this.html_url}}></a>
            <div class="card-header" id="issueTitle">
                {{this.title}}
            </div>

            <div class="card-body" id="card-body">
                <p class="card-text" id="issueBody">{{this.body}}</p>
            </div>
        </div>
      </div>

    {{/each}}
</div>



